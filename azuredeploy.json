{
  "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "Project Name": {
      "defaultValue": "projectname",
      "type": "string"
  }
  },
  "variables": {},
  "resources": [      
      {
          "type": "Microsoft.Web/serverfarms",
          "apiVersion": "2016-09-01",
          "name": "[concat(parameters('Project Name'), '_functionapp')]",
          "location": "Central US",
          "sku": {
              "name": "Y1",
              "tier": "Dynamic",
              "size": "Y1",
              "family": "Y",
              "capacity": 0
          },
          "kind": "functionapp",
          "properties": {
              "name": "[concat(parameters('Project Name'), '_functionapp')]",
              "perSiteScaling": false,
              "reserved": false,
              "targetWorkerCount": 0,
              "targetWorkerSizeId": 0
          }
      }
  ]
}